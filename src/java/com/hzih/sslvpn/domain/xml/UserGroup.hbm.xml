<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class table="user_group" name="com.hzih.sslvpn.domain.UserGroup">
        <!-- 复合主键使用composite-id标签 -->
        <composite-id>
            <!-- key-property标签表示哪一些属性对应复合主键 -->
            <key-property name="group_id" column="group_id" type="int" length="11" ></key-property>
            <key-property name="user_id" column="user_id" type="int" length="11" ></key-property>
        </composite-id>
    </class>
</hibernate-mapping>

